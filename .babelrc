{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    ["import", {"libraryName": "antd", "libraryDirectory": "es", "style": "css"}],
    "react-hot-loader/babel",
    "styled-components",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-syntax-dynamic-import"
  ]
}